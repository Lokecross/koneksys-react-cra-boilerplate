{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookstate/core": "^3.0.6",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "date-fns": "^2.19.0",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.13.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toast": "^1.0.1",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/query-string": "^6.3.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-persist": "^4.3.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "babel-jest": "^26.6.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "jest": "26.6.0",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "ts-jest": "^26.5.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "@app/(.*)": [
        "<rootDir>/src/app/$1"
      ],
      "@assets/(.*)": [
        "<rootDir>/src/assets/$1"
      ],
      "@models/(.*)": [
        "<rootDir>/src/models/$1"
      ],
      "@services/(.*)": [
        "<rootDir>/src/services/$1"
      ],
      "@components/(.*)": [
        "<rootDir>/src/presentation/components/$1"
      ],
      "@hooks/(.*)": [
        "<rootDir>/src/presentation/hooks/$1"
      ],
      "@pages/(.*)": [
        "<rootDir>/src/presentation/pages/$1"
      ],
      "@store/(.*)": [
        "<rootDir>/src/presentation/store/$1"
      ],
      "@styles/(.*)": [
        "<rootDir>/src/presentation/styles/$1"
      ],
      "@utils/(.*)": [
        "<rootDir>/src/presentation/utils/$1"
      ],
      "@app": [
        "<rootDir>/src/app/"
      ],
      "@assets": [
        "<rootDir>/src/assets/"
      ],
      "@models": [
        "<rootDir>/src/models/"
      ],
      "@services": [
        "<rootDir>/src/services/"
      ],
      "@components": [
        "<rootDir>/src/presentation/components/"
      ],
      "@hooks": [
        "<rootDir>/src/presentation/hooks/"
      ],
      "@pages": [
        "<rootDir>/src/presentation/pages/"
      ],
      "@store": [
        "<rootDir>/src/presentation/store/"
      ],
      "@styles": [
        "<rootDir>/src/presentation/styles/"
      ],
      "@utils": [
        "<rootDir>/src/presentation/utils/"
      ],
      "@hookstate/core": [
        "<rootDir>/node_modules/@hookstate/core/"
      ]
    }
  }
}
